<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCaraOuCoroa.PageJogo"
             BackgroundColor="#63BF8B"
             Title="Result. da jogada">
    
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="imgMoeda" Source="moeda_cara" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" />
            <ImageButton x:Name="btVoltar" Source="botao_voltar" Margin="20" BackgroundColor="Transparent" Clicked="btVoltar_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>